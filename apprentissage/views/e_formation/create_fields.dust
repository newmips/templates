                <i data-field="r_niveau_etudes" class="inline-help fa fa-info-circle" style="color: #1085EE"></i>
                {/inline_help}
            </label>
            <select class="ajax form-control" name="r_niveau_etudes" data-source="niveau_etudes" data-using="f_nom" required="">
                <option value="">
                    {#__ key="select.default" /}
                </option>
                {#r_niveau_etudes}
                <option value="{id}" selected="">{f_nom|string}</option>
                {/r_niveau_etudes}
            </select>
        </div>
    </div>
    <div data-field="f_reference" class="fieldLineHeight col-xs-12">
