<div id="fields" class="row">
    <div data-field="f_intitule" class="fieldLineHeight col-xs-12">
        <div class="form-group">
            <label for="f_intitule" class="required">
                {#__ key="entity.e_formation.f_intitule"/}&nbsp;
                {@inline_help field="f_intitule"}
                <i data-field="f_intitule" class="inline-help fa fa-info-circle" style="color: #1085EE"></i>
                {/inline_help}
            </label>
            <input class="form-control input" placeholder='{#__ key="entity.e_formation.f_intitule" /}' name="f_intitule" value="" type="text" maxlength="255" required="">
        </div>
    </div>
    <div data-field="f_niveau_etudes" class="fieldLineHeight col-xs-12">
        <div class="form-group">
            <label for="r_niveau_etudes" class="required">
