                        {@inline_help field="f_epargner_plus"}
                        <i data-field="f_epargner_plus" class="inline-help fa fa-info-circle" style="color: #1085EE"></i>
                        {/inline_help}
                    </label>
					
                	{^f_epargner_plus}
					<input class="form-control input" placeholder='{#__ key="entity.e_revenus_charges.f_epargner_plus" /}' name="f_epargner_plus" type="text" readonly="">
					{/f_epargner_plus}
					{#enum_radio.e_revenus_charges.f_epargner_plus}
					{@eq key=f_epargner_plus value="{.value}" }
					<input class="form-control input" placeholder='{#__ key="entity.e_revenus_charges.f_epargner_plus" /}' name="f_epargner_plus" value="{.translation}" type="text" readonly="">
					{/eq}
					{/enum_radio.e_revenus_charges.f_epargner_plus}
				</div>
            </div>
            <div data-field="f_combien_epargne" class="fieldLineHeight col-xs-12 col-sm-12 col-md-5" style="">
                <div class="form-group">
                    <label for="f_combien_epargne">
                        {#__ key="entity.e_revenus_charges.f_combien_epargne"/}&nbsp;
                        {@inline_help field="f_combien_epargne"}
                        <i data-field="f_combien_epargne" class="inline-help fa fa-info-circle" style="color: #1085EE"></i>
