                </div>
            </div>
        </div>
    </div>
    <div style="min-height: 75px;" class="emptySpaceColumn col-md-3 col-sm-3 col-xs-3 column">
        <div class="row">
            <div data-field="f_dep_solde_foyer" class="fieldLineHeight col-xs-12 col-md-12 col-sm-12" style="">
                <div class="form-group">
                    <label for="f_dep_solde_foyer">
                        {#__ key="entity.e_revenus_charges.f_dep_solde_foyer"/}&nbsp;
                        {@inline_help field="f_dep_solde_foyer"}
                        <i data-field="f_dep_solde_foyer" class="inline-help fa fa-info-circle" style="color: #1085EE"></i>
                        {/inline_help}
                    </label>
                    <input class="form-control input" data-custom-type="decimal" placeholder='{#__ key="entity.e_revenus_charges.f_dep_solde_foyer" /}' name="f_dep_solde_foyer" value="{f_dep_solde_foyer}" type="text" readonly="">
                </div>
            </div>
        </div>
    </div>
    <div style="min-height: 75px; margin-top: 25px;" class="emptySpaceColumn col-md-12 col-sm-12 col-xs-12 column">
        <div class="row" style="border: solid 1px #3c8dbc; padding: 15px; margin-left: 15px; margin-right: 15px;">
            <div data-field="f_correspondance" class="fieldLineHeight col-xs-12 col-sm-12 col-md-7" style="">
                <div class="form-group">
                    <label for="f_correspondance">
                        {#__ key="entity.e_revenus_charges.f_correspondance"/}&nbsp;
                        {@inline_help field="f_correspondance"}
                        <i data-field="f_correspondance" class="inline-help fa fa-info-circle" style="color: #1085EE"></i>
                        {/inline_help}
                    </label>
					{^f_correspondance}
					<input class="form-control input" placeholder='{#__ key="entity.e_revenus_charges.f_correspondance" /}' name="f_correspondance" type="text" readonly="">
					{/f_correspondance}
					{#enum_radio.e_revenus_charges.f_correspondance}
					{@eq key=f_correspondance value="{.value}" }
					<input class="form-control input" placeholder='{#__ key="entity.e_revenus_charges.f_correspondance" /}' name="f_correspondance" value="{.translation}" type="text" readonly="">
					{/eq}
					{/enum_radio.e_revenus_charges.f_correspondance}
				</div>
            </div>
            <div data-field="f_montant_epargne_annuelle" class="fieldLineHeight col-xs-12 col-sm-12 col-md-5" style="">
                <div class="form-group">
                    <label for="f_montant_epargne_annuelle">
