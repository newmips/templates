<h4 class="page-header">
	Revenus et Charges
</h4>
<table class="dataTable table">
	<thead>
		<tr>
			<th colspan="2">En K€</th>
			<th>Vous</th>
			<th>Conjoint</th>
			<th>Foyer</th>
			<th colspan="2">En K€</th>
			<th>Vous</th>
			<th>Conjoint</th>
			<th>Foyer</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td rowspan="5"><div class="title-rotate">REVENUS</div></td>
			<td>Professionnels</td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_pro_vous" value="{f_rev_pro_vous}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_pro_conj" value="{f_rev_pro_conj}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_pro_foyer" value="{f_rev_pro_foyer}" type="text" readonly=""></td>
			<td rowspan="5"><div class="title-rotate">DEPENSES</div></td>
			<td>Remboursement de crédits</td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_remb_vous" value="{f_dep_remb_vous}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_remb_conj" value="{f_dep_remb_conj}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_remb_foyer" value="{f_dep_remb_foyer}" type="text" readonly=""></td>
		</tr>
		<tr>
			<td style="display: none;"></td>
			<td>Loyers</td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_loyers_vous" value="{f_rev_loyers_vous}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_loyers_conj" value="{f_rev_loyers_conj}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_loyers_foyer" value="{f_rev_loyers_foyer}" type="text" readonly=""></td>
			<td style="display: none;"></td>
			<td>Pensions servies</td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_pensions_vous" value="{f_dep_pensions_vous}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_pensions_conj" value="{f_dep_pensions_conj}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_pensions_foyer" value="{f_dep_pensions_foyer}" type="text" readonly=""></td>
		</tr>
		<tr>
			<td style="display: none;"></td>
			<td>Dividendes / produits financiers</td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_div_vous" value="{f_rev_div_vous}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_div_conj" value="{f_rev_div_conj}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_div_foyer" value="{f_rev_div_foyer}" type="text" readonly=""></td>
			<td style="display: none;"></td>
			<td>Impôts (IR, IFI, autres)</td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_impots_vous" value="{f_dep_impots_vous}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_impots_conj" value="{f_dep_impots_conj}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_impots_foyer" value="{f_dep_impots_foyer}" type="text" readonly=""></td>
		</tr>
		<tr>
			<td style="display: none;"></td>
			<td>Autres</td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_autres_vous" value="{f_rev_autres_vous}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_autres_conj" value="{f_rev_autres_conj}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_autres_foyer" value="{f_rev_autres_foyer}" type="text" readonly=""></td>
			<td style="display: none;"></td>
			<td>Dépenses courantes</td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_courantes_vous" value="{f_dep_courantes_vous}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_courantes_conj" value="{f_dep_courantes_conj}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_courantes_foyer" value="{f_dep_courantes_foyer}" type="text" readonly=""></td>
		</tr>
		<tr>
			<td style="display: none;"></td>
			<td><b>Total annuel K€</b></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_total_vous" value="{f_rev_total_vous}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_total_conj" value="{f_rev_total_conj}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_rev_total_foyer" value="{f_rev_total_foyer}" type="text" readonly=""></td>
			<td style="display: none;"></td>
			<td><b>Total annuel K€</b></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_total_vous" value="{f_dep_total_vous}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_total_conj" value="{f_dep_total_conj}" type="text" readonly=""></td>
			<td><input class="form-control input" data-custom-type="decimal" placeholder='' name="f_dep_total_foyer" value="{f_dep_total_foyer}" type="text" readonly=""></td>
		</tr>
	</tbody>
</table>
<br>

<div id="fields" class="row">
	<div style="min-height: 75px;" class="emptySpaceColumn col-md-3 col-sm-3 col-xs-3 column">
		<div style="text-align: right">
			<br>SOLDE ANNUEL K€ (1-2) :
		</div>
	</div>
    <div style="min-height: 75px;" class="emptySpaceColumn col-md-3 col-sm-3 col-xs-3 column">
        <div class="row">
            <div data-field="f_dep_solde_vous" class="fieldLineHeight col-xs-12 col-md-12 col-sm-12" style="">
                <div class="form-group">
                    <label for="f_dep_solde_vous">
                        {#__ key="entity.e_revenus_charges.f_dep_solde_vous"/}&nbsp;
                        {@inline_help field="f_dep_solde_vous"}
                        <i data-field="f_dep_solde_vous" class="inline-help fa fa-info-circle" style="color: #1085EE"></i>
                        {/inline_help}
                    </label>
                    <input class="form-control input" data-custom-type="decimal" placeholder='{#__ key="entity.e_revenus_charges.f_dep_solde_vous" /}' name="f_dep_solde_vous" value="{f_dep_solde_vous}" type="text" readonly="">
                </div>
            </div>
        </div>
    </div>
    <div style="min-height: 75px;" class="emptySpaceColumn col-md-3 col-sm-3 col-xs-3 column">
        <div class="row">
            <div data-field="f_dep_solde_conj" class="fieldLineHeight col-xs-12 col-md-12 col-sm-12" style="">
