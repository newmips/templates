                    </label>
					<select style="width:100%;" class="form-control select" name="f_correspondance">
                        <option value="">
                            {#__ key="select.default" /}
                        </option>
                        {#enum_radio.e_revenus_charges.f_correspondance}
                        {@eq key=f_correspondance value="{.value}" }
                        <option value="{.value}" selected=""> {.translation} </option>
                        {:else}
                        <option value="{.value}"> {.translation} </option>
                        {/eq}
                        {/enum_radio.e_revenus_charges.f_correspondance}
                    </select>
				</div>
            </div>
            <div data-field="f_montant_epargne_annuelle" class="fieldLineHeight col-xs-12 col-sm-12 col-md-5" style="">
