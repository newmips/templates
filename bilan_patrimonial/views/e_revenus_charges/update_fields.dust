                        {@inline_help field="f_epargner_plus"}
                        <i data-field="f_epargner_plus" class="inline-help fa fa-info-circle" style="color: #1085EE"></i>
                        {/inline_help}
                    </label>
					<select style="width:100%;" class="form-control select" name="f_epargner_plus">
                        <option value="">
                            {#__ key="select.default" /}
                        </option>
                        {#enum_radio.e_revenus_charges.f_epargner_plus}
                        {@eq key=f_epargner_plus value="{.value}" }
                        <option value="{.value}" selected=""> {.translation} </option>
                        {:else}
                        <option value="{.value}"> {.translation} </option>
                        {/eq}
