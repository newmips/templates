                    <input class="form-control input" data-custom-type="decimal" placeholder='{#__ key="entity.e_revenus_charges.f_dep_solde_conj" /}' name="f_dep_solde_conj" value="{f_dep_solde_conj}" type="text">
                </div>
            </div>
        </div>
    </div>
    <div style="min-height: 75px;" class="emptySpaceColumn col-md-3 col-sm-3 col-xs-3 column">
        <div class="row">
            <div data-field="f_dep_solde_foyer" class="fieldLineHeight col-xs-12 col-md-12 col-sm-12" style="">
                <div class="form-group">
                    <label for="f_dep_solde_foyer">
                        {#__ key="entity.e_revenus_charges.f_dep_solde_foyer"/}&nbsp;
